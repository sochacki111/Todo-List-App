{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": ["airbnb-base", "plugin:node/recommended"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "linebreak-style": "off",
        "eol-last": "off"
    }
}
